\par
// Format price in Indian Rupees\par
function formatPrice(price) \{\par
    return '\'e2\'82\'b9' + price.toLocaleString('en-IN');\par
\}\par
\par
// Get products by category\par
function getProductsByCategory(category) \{\par
    return products.filter(product => product.category === category);\par
\}\par
\par
// Get product by ID\par
function getProductById(id) \{\par
    return products.find(product => product.id === parseInt(id));\par
\}\par
\par
// Get all categories\par
function getAllCategories() \{\par
    return [...new Set(products.map(product => product.category))];\par
\}\par
\par
// Search products\par
function searchProducts(query) \{\par
    const lowerQuery = query.toLowerCase();\par
    return products.filter(product => \par
        product.name.toLowerCase().includes(lowerQuery) ||\par
        product.description.toLowerCase().includes(lowerQuery) ||\par
        product.category.toLowerCase().includes(lowerQuery)\par
    );\par
\}\par
\par
// Sort products\par
function sortProducts(productList, sortBy) \{\par
    switch(sortBy) \{\par
        case 'price-low':\par
            return [...productList].sort((a, b) => a.price - b.price);\par
        case 'price-high':\par
            return [...productList].sort((a, b) => b.price - a.price);\par
        case 'name':\par
            return [...productList].sort((a, b) => a.name.localeCompare(b.name));\par
        default:\par
            return productList;\par
    \}\par
\}\par
}
